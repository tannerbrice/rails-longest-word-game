<h1>New Game</h1>
<ul>
  <% @letters.each do |letter| %>
    <li><%= letter %></li>
  <% end %>
</ul>

<form action="/score" method="POST">
  <input type="text" name="word" id="text-box">
  <input type="submit" value="Submit" id="button">
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
</form>

